    <ion-nav-back-button>
    </ion-nav-back-button>
  </ion-nav-bar>

  <ion-content class="padding">
    <p>
      {{item.date}}
    </p>
    <p>
      {{item.visittype}}
    </p>
    <p>
      {{item.client}}
    </p>
    <p>
      {{item.hisname}}
    </p>
    <p>
      {{item.hisphone}}
    </p>
