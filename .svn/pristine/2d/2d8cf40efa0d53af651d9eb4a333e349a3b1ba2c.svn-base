<ion-header-bar class="bar-positive" align-title="center">
  <i class="button" ng-click="goBack()">
    <span class="icon ion-ios-arrow-left"> Back </span>
  </i>
  <h1 class="title">我的签到</h1>
</ion-header-bar>
<ion-view>
  <ion-content class="padding">
    <div class="list">
      <div class="item item-input" style="background-color: #cacbcc;border-radius: 5px;">
        <!--style="background-color:cadetblue"-->
        <div ng-bind="datetime | date:'yyyy-MM-dd'" ng-click="openDatePicker()" style="width: auto;"></div>
        <i class="icon  ion-ios-search-strong placeholder-icon" style="font-size: 20px;float: right;"></i>
        <!--下次拜访时间： <input type="text" ng-model="$parent.next_visitTime">-->
      </div>
    </div>
  </ion-content>
</ion-view>

